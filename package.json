{
  "name": "@dollarcode/dollar-generators",
  "version": "3.1.4",
  "description": "generate boilerplates",
  "main": "dist/dg.js",
  "bin": {
    "dg": "dist/dg.js"
  },
  "scripts": {
    "develop": "npm run dev",
    "publish": "npm publish --access public",
    "bump": "npm version patch",
    "patch": "npm version patch && npm run update-cmd-version",
    "minor": "npm version minor && npm run update-cmd-version",
    "major": "npm version major && npm run update-cmd-version",
    "update": "npm i -g @dollarcode/dollar-generators",
    "patch-publish": "npm run patch && npm run build && npm run publish",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "dev:start": "npm-run-all build",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "copy-assets": "ts-node copy-express-files",
    "update-version": "ts-node update-cmd-version",
    "update-cmd-version": "ts-node update-cmd-version",
    "build": "npm-run-all clean tsc copy-assets",
    "test": "jest --watch",
    "commit-test": "jest"
  },
  "author": "Wes Dollar",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "copy-assets": "^1.0.3",
    "dotenv": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.1.0",
    "recursive-copy": "^2.0.13",
    "shelljs": "^0.8.4",
    "typescript": "^4.4.2",
    "yargs": "^17.1.1",
    "yargs-parser": "^20.2.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "@types/filesystem": "^0.0.32",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.13",
    "@types/pluralize": "0.0.29",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "cors": "^2.8.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-mdx": "^1.15.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.1.7",
    "husky": "^7.0.0",
    "jest": "^27.1.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1"
  },
  "keywords": [
    "generator",
    "express",
    "react",
    "create react app",
    "yeoman"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wesdollar/dollar-generators.git"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "./*.ts": "eslint --cache --fix",
    "src/**/*.ts": "eslint --cache --fix"
  }
}
